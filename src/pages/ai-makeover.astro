<script>
  // Structured Data
  const structuredData = {
    "@context": "https://schema.org",
    "@type": "Product",
    "name": "AI Makeover Package",
    "description": "Complete AI integration for existing businesses. Transform operations, customer experience, and growth with enterprise AI tools and expert implementation.",
    "brand": {
      "@type": "Brand",
      "name": "Skyabove"
    },
    "offers": {
      "@type": "Offer",
      "price": "3500",
      "priceCurrency": "USD",
      "priceSpecification": {
        "@type": "UnitPriceSpecification",
        "price": "3500",
        "priceCurrency": "USD",
        "billingDuration": "P1M",
        "billingIncrement": 1
      },
      "availability": "https://schema.org/InStock",
      "url": "https://skyabove.com/ai-makeover",
      "seller": {
        "@type": "Organization",
        "name": "Skyabove"
      }
    },
    "aggregateRating": {
      "@type": "AggregateRating",
      "ratingValue": "4.9",
      "reviewCount": "89"
    }
  };

  const script = document.createElement('script');
  script.type = 'application/ld+json';
  script.text = JSON.stringify(structuredData);
  document.head.appendChild(script);

  // GA4 Event Tracking
  function trackEvent(eventName: string, params = {}) {
    if (typeof gtag !== 'undefined') {
      gtag('event', eventName, params);
    }
  }

  trackEvent('page_view', {
    page_title: 'AI Makeover Package',
    page_location: window.location.href,
    page_path: '/ai-makeover'
  });

  // Form submission tracking
  const form = document.getElementById('contactForm');
  if (form) {
    form.addEventListener('submit', (e) => {
      e.preventDefault();

      const formData = new FormData(form as HTMLFormElement);
      const revenue = formData.get('revenue');

      trackEvent('generate_lead', {
        event_category: 'Form',
        event_label: 'AI Makeover Contact Form',
        value: 3500,
        currency: 'USD',
        revenue_range: revenue
      });

      trackEvent('conversion', {
        send_to: 'G-6CMBQ25C2Y/ai_makeover_form',
        value: 3500,
        currency: 'USD'
      });

      alert('Thank you! We\'ll contact you within 24 hours to schedule your free AI assessment.');
      form.reset();
    });

    // Track form start
    const formInputs = form.querySelectorAll('input, select, textarea');
    formInputs.forEach(input => {
      input.addEventListener('focus', () => {
        trackEvent('form_start', {
          event_category: 'Form',
          event_label: 'AI Makeover Form Interaction'
        });
      }, { once: true });
    });
  }

  // Track CTA clicks
  document.querySelectorAll('.btn-primary, .btn-secondary').forEach(button => {
    button.addEventListener('click', (e) => {
      const target = e.currentTarget as HTMLElement;
      const buttonText = target.textContent?.trim() || '';

      trackEvent('cta_click', {
        event_category: 'Engagement',
        event_label: buttonText,
        package_type: 'AI Makeover'
      });
    });
  });

  // Track phone clicks
  document.querySelectorAll('a[href^="tel:"]').forEach(link => {
    link.addEventListener('click', () => {
      trackEvent('phone_click', {
        event_category: 'Contact',
        event_label: 'AI Makeover Phone Click',
        phone_number: '(951) 760-0896'
      });
    });
  });

  // Track email clicks
  document.querySelectorAll('a[href^="mailto:"]').forEach(link => {
    link.addEventListener('click', () => {
      trackEvent('email_click', {
        event_category: 'Contact',
        event_label: 'AI Makeover Email Click'
      });
    });
  });
</script>
