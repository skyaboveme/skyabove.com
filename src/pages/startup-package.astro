  const form = document.getElementById('contactForm');
  if (form) {
    form.addEventListener('submit', (e) => {
      e.preventDefault();

      // Get form data
      const formData = new FormData(form as HTMLFormElement);
      const stage = formData.get('stage');

      // Track form submission as conversion
      trackEvent('generate_lead', {
        event_category: 'Form',
        event_label: 'Startup Package Contact Form',
        value: 3000,
        currency: 'USD',
        startup_stage: stage
      });

      // Track conversion
      trackEvent('conversion', {
        send_to: 'G-6CMBQ25C2Y/startup_package_form',
        value: 3000,
        currency: 'USD'
      });

      // Show success message
      alert('Thank you for your interest! We will contact you shortly.');
      form.reset();

      // Optional: Redirect to thank you page
      // window.location.href = '/thank-you';
    });

    // Track form field interactions
    const formInputs = form.querySelectorAll('input, select, textarea');
    formInputs.forEach(input => {
      input.addEventListener('focus', () => {
        trackEvent('form_start', {
          event_category: 'Form',
          event_label: 'Startup Package Form Interaction'
        });
      }, { once: true });
    });
  }

  // Track CTA button clicks
  document.querySelectorAll('.btn-primary, .btn-secondary').forEach(button => {
    button.addEventListener('click', (e) => {
      const target = e.currentTarget as HTMLElement;
      const buttonText = target.textContent?.trim() || '';
      const href = target.getAttribute('href') || '';

      trackEvent('cta_click', {
        event_category: 'Engagement',
        event_label: buttonText,
        button_location: getSectionName(target),
        link_url: href
      });
    });
  });

  // Track phone number clicks
  document.querySelectorAll('a[href^="tel:"]').forEach(link => {
    link.addEventListener('click', () => {
      trackEvent('phone_click', {
        event_category: 'Contact',
        event_label: 'Phone Number Click',
        phone_number: '(951) 760-0896'
      });
    });
  });

  // Track email clicks
  document.querySelectorAll('a[href^="mailto:"]').forEach(link => {
    link.addEventListener('click', () => {
      trackEvent('email_click', {
        event_category: 'Contact',
        event_label: 'Email Click',
        email_address: 'hello@skyabove.com'
      });
    });
  });

  // Track scroll depth
  let scrollDepths = [25, 50, 75, 100];
  let trackedDepths: number[] = [];

  window.addEventListener('scroll', () => {
    const scrollPercentage = Math.round(
      (window.scrollY / (document.documentElement.scrollHeight - window.innerHeight)) * 100
    );

    scrollDepths.forEach(depth => {
      if (scrollPercentage >= depth && !trackedDepths.includes(depth)) {
        trackedDepths.push(depth);
        trackEvent('scroll_depth', {
          event_category: 'Engagement',
          event_label: `${depth}% Scrolled`,
          value: depth
        });
      }
    });
  });

  // Track time on page
  let startTime = Date.now();
  let timeTracked = false;

  window.addEventListener('beforeunload', () => {
    if (!timeTracked) {
      const timeOnPage = Math.round((Date.now() - startTime) / 1000);
      trackEvent('time_on_page', {
        event_category: 'Engagement',
        event_label: 'Startup Package',
        value: timeOnPage
      });
      timeTracked = true;
    }
  });

  // Track section visibility
  const sections = document.querySelectorAll('section[class*="whats-included"], section[class*="ai-tools"], section[class*="pricing"]');
  const sectionObserver = new IntersectionObserver((entries) => {
    entries.forEach(entry => {
      if (entry.isIntersecting) {
        const sectionName = entry.target.className.split(' ')[0];
        trackEvent('section_view', {
          event_category: 'Engagement',
          event_label: sectionName
        });
        sectionObserver.unobserve(entry.target);
      }
    });
  }, { threshold: 0.5 });

  sections.forEach(section => sectionObserver.observe(section));

  // Helper function to get section name
  function getSectionName(element: HTMLElement): string {
    let current = element;
    while (current && current !== document.body) {
      if (current.tagName === 'SECTION') {
        return current.className.split(' ')[0] || 'unknown';
      }
      current = current.parentElement as HTMLElement;
    }
    return 'unknown';
  }
</script>
